<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:ParkenDD.Converters"
    xmlns:cimbalinoConverters="using:Cimbalino.Toolkit.Converters">

    <converters:TrimTextConverter x:Key="TrimTextConverter" />
    <converters:DistanceToParkingLotConverter x:Key="DistanceToParkingLotConverter" />
    <converters:DateTimeToStringConverter x:Key="DateTimeToStringConverter" />
    <converters:TimeSpanToDateTimeAxisLabelFormatConverter x:Key="TimeSpanToDateTimeAxisLabelFormatConverter" />
    <converters:ParkingLotStateToVisibilityConverter x:Key="ParkingLotStateToVisibilityConverter" />
    <converters:ParkingLotStateStringConverter x:Key="ParkingLotStateStringConverter" />
    <converters:IntGreaterZeroToVisibilityConverter x:Key="IntGreaterZeroToVisibilityConverter" />

    <cimbalinoConverters:NegativeBooleanConverter x:Key="NegativeBooleanConverter" />
    <cimbalinoConverters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    <cimbalinoConverters:BooleanToVisibilityConverter x:Key="NegativeBooleanToVisibilityConverter" InvertValue="True" />
    <cimbalinoConverters:StringToVisibilityConverter x:Key="StringToVisibilityConverter" />
    <cimbalinoConverters:NullToVisibilityConverter x:Key="NullToVisibilityConverter" />


</ResourceDictionary>
